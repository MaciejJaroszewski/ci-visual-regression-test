{
  "private": true,
  "name": "scs-with-reg-viz",
  "description": "A example repository of visual regression test using storybook-chrome-screenshot and reg-suit.",
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "visual-diff": "storybook-chrome-screenshot -p 9001 -c .storybook -o __screenshots__/current && reg-cli __screenshots__/current __screenshots__/expected __screenshots__/diff -R ./report.html",
    "visual-diff-update": "reg-cli __screenshots__/current __screenshots__/expected __screenshots__/diff -U"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.0.17"
  },
  "devDependencies": {
    "@storybook/react": "^3.3.9",
    "babel-core": "^6.26.0",
    "reg-cli": "^0.13.0",
    "reg-keygen-git-hash-plugin": "^0.7.0",
    "reg-notify-github-plugin": "^0.7.0",
    "reg-publish-s3-plugin": "^0.7.0",
    "reg-suit": "^0.7.0",
    "storybook-chrome-screenshot": "^1.0.0"
  },
  "author": "tsuyoshiwada",
  "license": "MIT"
}
